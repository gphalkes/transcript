	ibm/iso-8859_14-1998.ucm -c ibm/iso-8859_4-1998.ucm -c ibm/iso-8859_8-1999.ucm -c \
	ibm/iso-8859_15-1999.ucm -c ibm/iso-8859_5-1999.ucm -c ibm/iso-8859_9-1999.ucm

# Combine iso-2022 SBCS into a single package
iso-2022-sbcs: other/iso-2022-ascii.ucm -c other/iso-2022-88591.ucm -c other/iso-2022-88597.ucm -c \
	other/iso-2022-jisx0201kana.ucm -c other/iso-2022-jisx0201roman.ucm

